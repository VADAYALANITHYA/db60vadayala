 
block content 
  h1= title 
 
  if (toShow == null) 
    h2 No instance to delete 
  else 
    - let dogid = toShow.dogid
    - let breed = toShow.breed 
    - let price = toShow.price
    - let colour = toShow.colour
    - let actionid = "/resource/dog/" + dogid 
    script let actionid = "#{actionid}" 
    div 
    div.row 
      div(class='dogAttr mr-3') Dog ID is: 
      div #{dogid} 
    div.row 
      div(class='dogAttr mr-3') Dog Breed is: 
      div #{breed} 
    div.row 
      div(class='dogAttr mr-3') Dog Price is: 
      div #{price} 
    div.row 
      div(class='dogAttr mr-3') Dog Colour is: 
      div #{colour} 
 
    button(class="btn btn-danger m-2" onclick="deleteOnid()") Delete 
    button(class="btn btn-secondary m-2" onclick="cancel()") Cancel 
 
  script. 
    function cancel() { 
      // Go back to the home page for the costume collection 
      window.location.href='/dog' 
    } 
  script. 
    function deleteOnid() { 
      console.log("starting delete") 
      // delete first 
      fetch(actionid, { 
        method: 'DELETE'      
      }).then((resp) => { 
        return resp.json(); // or resp.text() or whatever the server sends 
      }).then((body) => { 
        alert("Delete succeded") 
        window.location.href='/dog' 
        // TODO handle body 
      }).catch((error) => { 
        // TODO handle error 
        alert("Delete failed " + error ) 
      }); 
    } 